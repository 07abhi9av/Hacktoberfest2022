#include <bits/stdc++.h>

using namespace std;

typedef long long ll;

ll elementFreq(ll arr[], int n)

{

    unordered_map<int, int> mp;

    for (int i = 0; i < n; i++)

        mp[arr[i]]++;

    for (auto x : mp)

        if(x.second==1)

            return x.first;

    return 0;

}

int main()

{

    ll t;

    cin >> t;

    while (t--) {

        ll n;

        cin >> n;

        ll arr[n] = {0};

        for(ll i = 0;i<n;i++)

            cin>>arr[i];

        cout<<elementFreq(arr, n)<<endl;

    }

    return 0;

}
