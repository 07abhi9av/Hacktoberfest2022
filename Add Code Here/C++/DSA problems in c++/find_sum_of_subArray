#include<iostream>
using namespace std;
int main(){
int n=6;
int arr[]={1,4,20,3,10,5};
int sum=33;
int s=0,curr=0;
for(int i=0;i<n;i++){
    curr+=arr[i];
    while(sum>curr){
        sum=arr[s];
        s++;
    }
    if(sum==curr){
        cout<<true;
    }
}
 if(sum==curr){
    cout<<false;
 }
}
