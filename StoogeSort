#include <iostream>
using namespace std;

void stoogeSort(int arr[], int low, int high) {
    if (low >= high) {
        return;
    }

    if (arr[low] > arr[high]) {
        // Swap if the left element is greater than the right element
        swap(arr[low], arr[high]);
    }

    if (high - low + 1 > 2) {
        int third = (high - low + 1) / 3;

        // Recursively sort first two-thirds and last two-thirds of the array
        stoogeSort(arr, low, high - third);
        stoogeSort(arr, low + third, high);
        stoogeSort(arr, low, high - third);
    }
}

int main() {
    int n;

    // Get the number of elements from the user
    cout << "Enter the number of elements: ";
    cin >> n;

    int arr[n];

    // Get the elements from the user
    cout << "Enter the elements:\n";
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }

    // Sort the array using Stooge Sort
    stoogeSort(arr, 0, n - 1);

    // Output the sorted array
    cout << "Sorted array: ";
    for (int i = 0; i < n; i++) {
        cout << arr[i] << " ";
    }

    return 0;
}
